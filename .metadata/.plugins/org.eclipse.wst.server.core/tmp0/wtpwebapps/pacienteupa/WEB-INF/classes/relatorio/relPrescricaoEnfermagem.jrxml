	<queryString>
		<![CDATA[Select
 D.NUM_ORDEM,
 LPAD(cast(A.ISN_INTERNACAO_PRESC as varchar),6,'0') ISN_INTERNACAO_PRESC,
 LPAD(E3.NUM_PRONTUARIO,6,'0') NUM_PRONTUARIO,
 case when E3.DAT_NASCIMENTO is null then '*******' else medpoint.IDADE(E3.DAT_NASCIMENTO) end idade,
 UPPER(E3.DSC_NOME) DSC_NOME,
 UPPER(A.DSC_OBS) DSC_OBS,
 TO_CHAR(A.DAT_CADASTRO,'DD/MM/YYYY') DAT_CADASTRO,
 UPPER(I.DSC_CLINICA) DSC_CLINICA,
 UPPER(F.DSC_ENFERMARIA) DSC_ENFERMARIA,
 case when G.ISN_LEITO is null then 'EXTRA' else UPPER(G.DSC_LEITO) end dsc_leito,
 (case when (B.DSC_OBS is null or B.DSC_OBS = '') then '' else UPPER(B.DSC_OBS) end) || ' ' || UPPER(C.DSC_AGENDAMENTO) as dsc_prescricao,
 UPPER(C.DSC_AGENDAMENTO) DSC_AGENDAMENTO,
 case when (A.DSC_OBS is null or A.DSC_OBS = '') then '-' else UPPER(A.DSC_OBS) end dsc_obs,
 UPPER(D.DSC_TIPO_PRESCRICAO) DSC_TIPO_PRESCRICAO,
 B.QTD_REFERENCIA QTD_REFERENCIA,
 case when (B.DSC_APRAZAMENTO is null or B.DSC_APRAZAMENTO = '') then '' else UPPER(B.DSC_APRAZAMENTO) end DSC_APRAZAMENTO ,
 H.DSC_USUARIO AS NOM_PROFISSIONAL,
 H.NUM_CONSELHO,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 13) as dsc_unidade,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 19) as logo_unidade
From
 MEDPOINT.T_INTERNACAO_PRESC A
 INNER JOIN MEDPOINT.T_ITEM_INTERNACAO_PRESC B ON A.ISN_INTERNACAO_PRESC = B.ISN_INTERNACAO_PRESC
 INNER JOIN MEDPOINT.T_PRESCRICAO C ON B.ISN_PRESCRICAO = C.ISN_PRESCRICAO
 INNER JOIN MEDPOINT.T_TIPO_PRESCRICAO D ON B.ISN_TIPO_PRESCRICAO = D.ISN_TIPO_PRESCRICAO
 INNER JOIN MEDPOINT.T_INTERNACAO_LEITO E1 ON A.ISN_INTERNACAO_LEITO = E1.ISN_INTERNACAO_LEITO
 INNER JOIN MEDPOINT.T_INTERNACAO E2 ON E1.ISN_INTERNACAO = E2.ISN_INTERNACAO
 INNER JOIN MEDPOINT.T_PACIENTE E3 ON E2.ISN_PACIENTE = E3.ISN_PACIENTE
 INNER JOIN MEDPOINT.T_ENFERMARIA F ON E1.ISN_ENFERMARIA = F.ISN_ENFERMARIA
 LEFT JOIN MEDPOINT.T_LEITO G ON E1.ISN_LEITO = G.ISN_LEITO
 INNER JOIN SEGURANCA.T_USUARIO H ON A.ISN_PROFISSIONAL = H.ISN_USUARIO
 INNER JOIN MEDPOINT.T_CLINICA I ON E1.ISN_CLINICA = I.ISN_CLINICA
Where
 A.ISN_INTERNACAO_PRESC = $P{p1}::integer
Order by
 D.NUM_ORDEM, UPPER(D.DSC_TIPO_PRESCRICAO)]]>
	</queryString>
