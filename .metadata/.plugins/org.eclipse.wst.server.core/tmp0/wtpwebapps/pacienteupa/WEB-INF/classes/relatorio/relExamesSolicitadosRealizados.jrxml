	<queryString>
		<![CDATA[select e.dsc_exame,
	count(*) as solicitados,
	(select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
	(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 13) AS DSC_UNIDADE,
	sum(CASE WHEN es.isn_exame_situacao <> 2 THEN 0 ELSE 1 END) as realizados,
	gr.dsc_exame_grupo,
	gr.isn_exame_grupo
	from
		medpoint.t_exame e
	inner join
		medpoint.t_exame_solic es on e.isn_exame = es.isn_exame
	left join
		medpoint.t_exame_grupo gr on gr.isn_exame_grupo = e.isn_exame_grupo

	where isn_exame_situacao <> 3 and dat_solicitacao between ($P{dataInicio}) and ($P{dataFim})
	group by e.dsc_exame, gr.dsc_exame_grupo, gr.isn_exame_grupo
	order by gr.dsc_exame_grupo]]>
	</queryString>
