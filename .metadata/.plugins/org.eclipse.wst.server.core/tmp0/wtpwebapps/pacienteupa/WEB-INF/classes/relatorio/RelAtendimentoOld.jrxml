	<queryString>
		<![CDATA[select distinct(i.isn_internacao),p.num_prontuario,p.dsc_nome,p.num_rg,p.num_rg,p.dsc_orgao_emissao_rg,p.num_cep,
       i.dat_cadastro, p.dat_nascimento,i.isn_internacao,
       s.dsc_sexo,i.dsc_endereco,i.dsc_endereco,p.dsc_mae,i.dsc_bairro,
       i.num_ddd_1,i.num_telefone_1,ir.dsc_queixa,f.dsc_fluxograma,
       ir.dsc_discriminador,ir.dsc_sato2,ir.dsc_glassgow,ir.dsc_glicemia,ir.dsc_regua_dor,ir.dsc_fc,ir.dsc_temperatura,
       ri.dsc_risco,u.dsc_usuario,ir.hor_registro,ia.dsc_atendimento,ia.dsc_pa,ir.dsc_peso,
       ia.flg_acidente,ia.flg_agressao,ia.hipoteses_diagnostico,ia.comorbidade, ia.isn_internacao_atendimento,
       dm.dsc_destino_medico,uia.dsc_usuario as profissional,uia.num_conselho,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 13) as dsc_unidade
       from medpoint.t_internacao i
	    inner join medpoint.t_paciente p on i.isn_paciente = p.isn_paciente
	    inner join medpoint.t_sexo s on p.isn_sexo = s.isn_sexo
            inner join medpoint.t_internacao_leito il on i.isn_internacao = i.isn_internacao
            inner join (select * from medpoint.t_internacao_risco where
            isn_internacao_risco =(select max(isn_internacao_risco)from medpoint.t_internacao_risco where isn_internacao = $P{p1}))
            ir on i.isn_internacao = ir.isn_internacao
	    inner join medpoint.t_fluxograma f on ir.isn_fluxograma = f.isn_fluxograma
	    inner join medpoint.t_risco ri on ir.isn_risco = ri.isn_risco
	    inner join seguranca.t_usuario u on ir.isn_usuario = u.isn_usuario
	    inner join (select * from medpoint.t_internacao_atendimento where
            isn_internacao_atendimento =(select max(isn_internacao_atendimento) from medpoint.t_internacao_atendimento where isn_internacao = $P{p1}))
            ia on ia.isn_internacao = ir.isn_internacao
	    inner join seguranca.t_usuario uia on ia.isn_profissional = uia.isn_usuario
            left join medpoint.t_destino_medico dm on ia.isn_clinica_destino = dm.isn_destino_medico
            where il.dat_alta is null]]>
	</queryString>
