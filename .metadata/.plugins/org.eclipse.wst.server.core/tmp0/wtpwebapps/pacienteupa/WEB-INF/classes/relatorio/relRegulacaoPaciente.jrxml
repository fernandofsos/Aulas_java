	<queryString>
		<![CDATA[select 	i.isn_internacao,p.num_prontuario,p.dsc_nome,p.num_rg, p.dsc_orgao_emissao_rg,p.num_cep,
		i.dat_cadastro, p.dat_nascimento,s.dsc_sexo,i.dsc_endereco,i.dsc_endereco,p.dsc_mae,i.dsc_bairro,
		medpoint.idade2(p.dat_nascimento, i.dat_cadastro) as idade,i.num_ddd_1,i.num_telefone_1,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 13) as dsc_unidade,
		it.cod_regulacao_est, it.cod_regulacao_mun, it.dat_regulacao_est, it.dat_regulacao_mun,
		it.cod_regulacao_samu, it.dat_regulacao_samu,
		usrest.dsc_usuario as usuarioestado, usrmun.dsc_usuario as usuariomunicipio,
		usrsamu.dsc_usuario as usuariosamu
from medpoint.t_internacao_trans it
		inner join medpoint.t_internacao i on it.isn_internacao = i.isn_internacao
		inner join medpoint.t_paciente p on p.isn_paciente = i.isn_paciente
		inner join medpoint.t_sexo s on s.isn_sexo = p.isn_sexo
		left join  seguranca.t_usuario usrest on usrest.isn_usuario = it.isn_usuario_reg_est
		left join  seguranca.t_usuario usrmun on usrmun.isn_usuario = it.isn_usuario_reg_mun
		left join  seguranca.t_usuario usrsamu on usrsamu.isn_usuario = it.isn_usuario_reg_samu
where it.isn_internacao_trans = $P{p1}]]>
	</queryString>
