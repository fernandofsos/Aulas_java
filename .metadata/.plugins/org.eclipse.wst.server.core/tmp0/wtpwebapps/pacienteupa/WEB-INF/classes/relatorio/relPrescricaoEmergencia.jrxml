	<queryString>
		<![CDATA[select
      distinct(i.isn_internacao),p.num_prontuario,p.dsc_nome,p.num_rg,p.num_rg,p.dsc_orgao_emissao_rg,p.num_cep,
       i.dat_cadastro, p.dat_nascimento,i.isn_internacao,
       s.dsc_sexo,i.dsc_endereco,i.dsc_endereco,p.dsc_mae,i.dsc_bairro,
       i.num_ddd_1,i.num_telefone_1,
       i.isn_internacao,
       medpoint.idade2(p.dat_nascimento, i.dat_cadastro) as idade,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 13) as dsc_unidade,
CASE WHEN (ppi.ISN_PRESCRICAO_PEP_IS is null and (pro.flg_mpp is null or pro.flg_mpp is false or ppi.ISN_TIPO_PRESCRICAO = 43))
		THEN upper(ppi.DSC_PRESCRICAO) || ' ' ||
			CASE WHEN ppi.FLG_SE_NECESSARIO = 'S'
				THEN ' - ** SE NECESSÁRIO **' ELSE ''
			END
			||' '||
			CASE WHEN ppi.FLG_ACM = 'S'
				THEN ' - ** ACM **' ELSE ''
			END
			||' '||
			CASE WHEN D.DIAS is null THEN '' ELSE '(**D'||D.DIAS||'**)'
			END
		ELSE NULL
		END || case when (ppi.dsc_complemento is null or ppi.dsc_complemento = '') then '' else '  |  ' || ppi.dsc_complemento end || coalesce(ppi.contador_antibiotico_relatorio, '') as dsc_prescricao,
       CASE WHEN (ppi.ISN_PRESCRICAO_PEP_IS is null and pro.flg_mpp is true and ppi.ISN_TIPO_PRESCRICAO != 43)
		THEN upper(ppi.DSC_PRESCRICAO) || ' ' ||
		CASE WHEN ppi.FLG_SE_NECESSARIO = 'S'
		THEN ' - ** SE NECESSÁRIO **' ELSE ''
		END ||' '||
		CASE WHEN ppi.FLG_ACM = 'S'
		THEN ' - ** ACM **' ELSE ''
		END ||' '||
		CASE WHEN D.DIAS is null
		THEN ''
		ELSE '(**D'||D.DIAS||'**)'
		END
	ELSE NULL
	END || case when (ppi.dsc_complemento is null or ppi.dsc_complemento = '') then '' else '  |  ' || ppi.dsc_complemento end || coalesce(ppi.contador_antibiotico_relatorio, '') as dsc_prescricao_mpp,

       CASE WHEN (ppi.ISN_PRESCRICAO_PEP_IS is not null and (pro.flg_mpp is null or pro.flg_mpp is false or ppi.ISN_TIPO_PRESCRICAO = 43))
		THEN upper(ppi.DSC_PRESCRICAO)|| ' ' ||
		CASE WHEN ppi.FLG_SE_NECESSARIO = 'S' THEN ' - ** SE NECESSÁRIO **' ELSE ''
		END ||' '||
		CASE WHEN ppi.FLG_ACM = 'S' THEN ' - ** ACM **' ELSE ''
		END ||' '||
		CASE WHEN D.DIAS is null THEN '' ELSE '(**D'||D.DIAS||'**)'
		END ||'  '|| '(SUSPENSO)' ELSE NULL
		END ||
	case when (ppi.dsc_complemento is null or ppi.dsc_complemento = '') then '' else '  |  ' || ppi.dsc_complemento end || coalesce(ppi.contador_antibiotico_relatorio, '') as DSC_DESCRICAO_SUSPENSAO,

       CASE WHEN (ppi.ISN_PRESCRICAO_PEP_IS is not null and pro.flg_mpp is true and ppi.ISN_TIPO_PRESCRICAO != 43) THEN upper(ppi.DSC_PRESCRICAO)|| ' ' || CASE WHEN ppi.FLG_SE_NECESSARIO = 'S' THEN ' - ** SE NECESSÁRIO **' ELSE '' END ||' '|| CASE WHEN ppi.FLG_ACM = 'S' THEN ' - ** ACM **' ELSE '' END ||' '|| CASE WHEN D.DIAS is null THEN '' ELSE '(**D'||D.DIAS||'**)' END ||'  '|| '(SUSPENSO)' ELSE NULL END || case when (ppi.dsc_complemento is null or ppi.dsc_complemento = '') then '' else '  |  ' || ppi.dsc_complemento end ||
coalesce(ppi.contador_antibiotico_relatorio, '') as DSC_DESCRICAO_SUSPENSAO_MPP,

       ppi.dat_cadastro as dat_cad_prescricao,
       usr.dsc_usuario as nom_profissional,usrc.dsc_usuario as nom_profissional_concl,
       ppi.isn_prescricao_pep_item,
       ppi.isn_prescricao_pep_is,
       pp.dat_prescricao,
       pp.dat_conclusao,
       pp.dat_apz,
       upper(tp.DSC_TIPO_PRESCRICAO) DSC_TIPO_PRESCRICAO,
       tp.num_ordem,
       ie.isn_internacao_evolucao,
 ppi.ISN_TIPO_PRESCRICAO::integer,
 ppi.ISN_DIETA,
(case when dz.dat_inicial is null then '' else (' / (D' ||
 regexp_replace(medpoint.seconds_dd_hh_mi((extract (epoch from age(to_char((case when dz.dat_final is null then $P{data_atual}::timestamp else dz.dat_final end),'dd/MM/yyyy HH24:MI:SS')::timestamp, to_char(dz.dat_inicial,'dd/MM/yyyy HH24:MI:SS')::timestamp))::integer))
 ,'dias', '-') || ')') end) as QTD_DIAS_DIETA_ZERO
from medpoint.t_prescricao_pep_item ppi
    inner join medpoint.t_prescricao_pep pp on ppi.isn_prescricao_pep = pp.isn_prescricao_pep
    inner join medpoint.t_internacao i on pp.isn_internacao = i.isn_internacao
    inner join medpoint.t_paciente p on p.isn_paciente = i.isn_paciente
    inner join medpoint.t_sexo s on s.isn_sexo = p.isn_sexo
    inner join seguranca.t_usuario usr on usr.isn_usuario = pp.isn_profissional
    INNER JOIN medpoint.T_TIPO_PRESCRICAO tp ON  ppi.ISN_TIPO_PRESCRICAO = tp.ISN_TIPO_PRESCRICAO
    left  join seguranca.t_usuario usrc on usrc.isn_usuario = pp.isn_profissional_concl
    left join medpoint.t_internacao_evolucao ie on ie.isn_prescricao_pep = pp.isn_prescricao_pep
    LEFT JOIN
    (select
        a.isn_internacao,
        b.isn_produto_almox,
        date(date_trunc('day', current_date)) - date(date_trunc('day', a.dat_inicio)) dias
    from
        medpoint.t_ficha a,
        medpoint.t_ficha_item b
    where
        a.isn_ficha = b.isn_ficha and
        date(date(a.dat_inicio) + b.num_dias_validade_ficha::integer) >= date(date_trunc('day', current_date)) and a.isn_internacao = $P{p2}::numeric
         ) as D ON ppi.isn_produto_almox = D.isn_produto_almox
    left join almox.t_produto pro on pro.isn_produto = ppi.isn_produto_almox
    LEFT JOIN MEDPOINT.T_PRESCRICAO_PEP_ITEM_DIETA_ZERO DZ ON DZ.ISN_PRESCRICAO_PEP_ITEM_DIETA_ZERO = ppi.ISN_PRESCRICAO_PEP_ITEM_DIETA_ZERO
where ppi.isn_prescricao_pep = $P{p1} and ppi.flg_ativo = 'S' order by tp.num_ordem asc]]>
	</queryString>
