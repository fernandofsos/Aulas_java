	<queryString>
		<![CDATA[select
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 13) as dsc_unidade,

       ES.ISN_EXAME_SOLIC,
       PA.DSC_NOME,
       EX.DSC_EXAME,
       ES.DAT_SOLICITACAO,
       ES.dat_Realizacao,

       PA.DAT_CADASTRO, PA.DSC_MAE, PA.DSC_BAIRRO, PA.NUM_DDD_1, PA.NUM_TELEFONE_1, PA.NUM_RG,
        PA.DSC_ENDERECO, PA.NUM_CEP, PA.DAT_NASCIMENTO, PA.NUM_PRONTUARIO, SE.DSC_SEXO,
        medpoint.idade2(PA.dat_nascimento,IT.dat_cadastro) as idade
FROM MEDPOINT.T_EXAME_SOLIC ES
     LEFT JOIN MEDPOINT.T_EXAME EX ON ES.ISN_EXAME = EX.ISN_EXAME
     LEFT JOIN MEDPOINT.T_INTERNACAO IT ON IT.ISN_INTERNACAO = ES.ISN_INTERNACAO
     LEFT JOIN MEDPOINT.T_PACIENTE PA ON PA.ISN_PACIENTE = IT.ISN_PACIENTE
     LEFT JOIN medpoint.t_sexo SE on PA.isn_sexo = SE.isn_sexo
WHERE ES.ISN_EXAME_SOLIC IN
($P{parameter1},  $P{parameter2},  $P{parameter3},  $P{parameter4},  $P{parameter5},
 $P{parameter6},  $P{parameter7},  $P{parameter8},  $P{parameter9},  $P{parameter10},
 $P{parameter11}, $P{parameter12}, $P{parameter13}, $P{parameter14}, $P{parameter15},
 $P{parameter16}, $P{parameter17}, $P{parameter18}, $P{parameter19}, $P{parameter20},
 $P{parameter21}, $P{parameter22}, $P{parameter23}, $P{parameter24}, $P{parameter25},
 $P{parameter26}, $P{parameter27}, $P{parameter28}, $P{parameter29}, $P{parameter30})]]>
	</queryString>
