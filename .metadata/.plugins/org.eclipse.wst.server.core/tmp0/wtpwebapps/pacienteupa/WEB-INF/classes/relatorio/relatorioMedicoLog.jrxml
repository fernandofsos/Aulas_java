	<queryString>
		<![CDATA[SELECT DISTINCT(I.ISN_INTERNACAO),
                P.NUM_PRONTUARIO AS PRONTUARIO,
                P.DSC_NOME AS NOME,
		P.DAT_NASCIMENTO,
		MEDPOINT.IDADE2(P.DAT_NASCIMENTO, I.DAT_CADASTRO) AS IDADE,
		S.DSC_SEXO AS SEXO,
		I.DSC_ENDERECO AS ENDERECO,
		I.DSC_BAIRRO AS BAIRRO,
		I.ISN_MUNICIPIO AS MUNICIPIO,
		I.ISN_UF AS ESTADO,
		I.NUM_CEP AS CEP,
		US.DSC_USUARIO AS USUARIO,
		IT.DAT_TRANS,
		IT.DSC_RESUMO_CLINICO,
		IT.DSC_EXAME AS EXAME,
		IT.DSC_TERAPEUTICA AS TERAPEUTICA,
		IT.DSC_REFERENCIA AS REFERENCIA,
		IT.DSC_OBS AS OBSERVACAO,
		IT.ISN_USUARIO AS USUARIO,
		US.DSC_LOGIN AS LOGIN,
		M.DSC_MUNICIPIO AS DSC_MUNICIPIO,
		U.DSC_UF AS DSC_UF,
		IT.DSC_HIPOTESE_DIAGNOSTICA,
		IT.DSC_JUSTIFICATIVA_OUTROS,
		PT.DSC_PERFIL_TRANSF_ITEM,
		cid.cod_cid10,
(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 10) AS ENDERECO_COMPLETO,
(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 13) AS DSC_UNIDADE
FROM MEDPOINT.T_INTERNACAO I INNER JOIN MEDPOINT.T_PACIENTE P ON I.ISN_PACIENTE = P.ISN_PACIENTE
			     INNER JOIN MEDPOINT.T_INTERNACAO_TRANS_LOG IT ON I.ISN_INTERNACAO = IT.ISN_INTERNACAO
			     INNER JOIN MEDPOINT.T_SEXO S ON P.ISN_SEXO = S.ISN_SEXO
			     INNER JOIN MEDPOINT.T_MUNICIPIO M ON I.ISN_MUNICIPIO = M.ISN_MUNICIPIO
			     INNER JOIN MEDPOINT.T_UF U ON U.ISN_UF = M.ISN_UF
			     LEFT JOIN MEDPOINT.T_PERFIL_TRANSF_ITEM PT ON PT.ISN_PERFIL_TRANSF_ITEM = IT.ISN_PERFIL_TRANSF_ITEM
			     INNER JOIN SEGURANCA.T_USUARIO US ON IT.ISN_USUARIO = US.ISN_USUARIO
			left join medpoint.t_cid10 cid on cid.isn_cid10 = it.isn_cid10
		WHERE IT.ISN_INTERNACAO_TRANS_LOG = $P{p1}]]>
	</queryString>
