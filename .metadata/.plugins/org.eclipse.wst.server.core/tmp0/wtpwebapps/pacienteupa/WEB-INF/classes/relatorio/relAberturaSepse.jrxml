	<queryString>
		<![CDATA[select distinct(i.isn_internacao),p.num_prontuario,p.dsc_nome,p.num_rg,p.num_rg,p.dsc_orgao_emissao_rg,i.num_cep,
       i.dat_cadastro, p.dat_nascimento,i.isn_internacao,i.hor_chegada,
       s.dsc_sexo,i.dsc_endereco,i.dsc_endereco,p.dsc_mae,i.dsc_bairro,
       i.num_ddd_1,i.num_telefone_1,ir.dsc_queixa,f.dsc_fluxograma,ia.dsc_hip_diag_odonto,ia.dsc_hda_odonto,ia.dsc_comorbidade_odonto,
       ir.dsc_discriminador,ir.dsc_sato2,ir.dsc_glassgow,ir.dsc_glicemia,ir.dsc_regua_dor,ir.dsc_fc,ir.dsc_temperatura,
       ri.dsc_risco,u.dsc_usuario,ir.hor_registro,ir.dat_registro,ia.dsc_atendimento,ia.dsc_pa,ir.dsc_peso,ia.isn_cid10,
       ia.flg_acidente,ia.flg_agressao,ia.hipoteses_diagnostico,ia.comorbidade,to_char(ia.dat_inicio_med,'dd/MM/yyyy HH24:MI') as dat_inicio_med ,to_char(ia.dat_inicio_odonto, 'dd/MM/yyyy HH24:MI') as dat_inicio_odonto, i.isn_internacao,
       medpoint.idade2(p.dat_nascimento,i.dat_cadastro) as idade,
       dm.dsc_destino_medico,uia.dsc_usuario as usuario_medico,uodon.dsc_usuario as usuario_odonto,uia.num_conselho,uodon.num_conselho as num_conselho_odonto,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 13) as dsc_unidade,
       p.flg_alergia,ir.flg_risco_queda,p.tipo_alergia,ir.flg_dengue,ir.dat_sintoma_dengue, ia.isn_protocolo_sepse,
       ps.cod_tela1, ps.cod_tela2, ps.outros_sinais_alerta, ir.flg_chikungunya,ir.dat_sintoma_chikungunya
       from medpoint.t_internacao i
	    inner join medpoint.t_paciente p on i.isn_paciente = p.isn_paciente
	    inner join medpoint.t_sexo s on p.isn_sexo = s.isn_sexo
            left join (select * from medpoint.t_internacao_risco where
            isn_internacao_risco =(select max(isn_internacao_risco)from medpoint.t_internacao_risco where isn_internacao = $P{p1}::integer))
            ir on i.isn_internacao = ir.isn_internacao
	    left join medpoint.t_fluxograma f on ir.isn_fluxograma = f.isn_fluxograma
	    left join medpoint.t_risco ri on ir.isn_risco = ri.isn_risco
	    left join seguranca.t_usuario u on ir.isn_usuario = u.isn_usuario
	    left join (select * from medpoint.t_internacao_atendimento where
            isn_internacao_atendimento =(select max(isn_internacao_atendimento) from medpoint.t_internacao_atendimento where isn_internacao = $P{p1}::integer))
            ia on ia.isn_internacao = ir.isn_internacao
	    left join seguranca.t_usuario uia on ia.isn_usuario_med = uia.isn_usuario
	    left join seguranca.t_usuario uodon on ia.isn_usuario_odonto = uodon.isn_usuario
            left join medpoint.t_destino_medico dm on ia.isn_clinica_destino = dm.isn_destino_medico
            left join medpoint.t_protocolo_sepse ps on ps.isn_protocolo_sepse = ia.isn_protocolo_sepse
            where i.isn_internacao = $P{p1}::integer]]>
