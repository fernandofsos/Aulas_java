	<queryString>
		<![CDATA[SELECT USR.DSC_USUARIO AS NOMEMEDICO, USR.NUM_CONSELHO AS NUMCONSELHO, CNS.SGL_CONSELHO AS SGLCONSELHO,
	UF.SGL_UF AS UF, PAC.DSC_NOME AS NOMEPACIENTE, DOC.DSC_TEXTO AS PRESCRICAO, CURRENT_DATE AS DT_ATUAL,
	I.DSC_ENDERECO || ', ' ||  I.DSC_BAIRRO || ', ' || MUN.DSC_MUNICIPIO AS ENDERECO,DOC.ISN_INTERNACAO_DOC,
PAC.NUM_PRONTUARIO AS PRONTUARIO,
(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 10) AS ENDERECO_COMPLETO,
	(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 11) AS ENDERECO_REDUZIDO,
	(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 12) AS TELEFONE_UPA,
	(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 13) AS DSC_UNIDADE
FROM MEDPOINT.T_INTERNACAO_DOC AS DOC
	INNER JOIN MEDPOINT.T_INTERNACAO AS I ON I.ISN_INTERNACAO = DOC.ISN_INTERNACAO
	INNER JOIN MEDPOINT.T_PACIENTE AS PAC ON PAC.ISN_PACIENTE = I.ISN_PACIENTE
	INNER JOIN SEGURANCA.T_USUARIO AS USR ON USR.ISN_USUARIO = DOC.ISN_USUARIO
	LEFT JOIN SEGURANCA.T_CONSELHO AS CNS ON CNS.ISN_CONSELHO = USR.ISN_CONSELHO
	LEFT JOIN SEGURANCA.V_UF AS UF ON UF.ISN_UF = USR.ISN_UF_CONSELHO
	LEFT JOIN MEDPOINT.T_MUNICIPIO AS MUN ON MUN.ISN_MUNICIPIO = I.ISN_MUNICIPIO
WHERE I.ISN_INTERNACAO = $P{P1}
	AND DOC.ISN_INTERNACAO_DOC = $P{P2}]]>
	</queryString>
