	<queryString>
		<![CDATA[select pac.dsc_nome as nomepaciente, doc.qtd_dias as nr_dias, usr.dsc_usuario as nomemedico,
usr.num_conselho as numconselho, cns.sgl_conselho as sglconselho,doc.dsc_texto,
(select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
(select vlr_parametro from medpoint.t_parametro where isn_parametro = 23) as nome_cidade,doc.isn_internacao_doc,
(select vlr_parametro from medpoint.t_parametro where isn_parametro = 13) as dsc_unidade
from medpoint.t_internacao_doc doc
inner join medpoint.t_internacao i on i.isn_internacao = doc.isn_internacao
inner join medpoint.t_paciente pac on pac.isn_paciente = i.isn_paciente
inner join seguranca.t_usuario usr on usr.isn_usuario = doc.isn_usuario
left join seguranca.t_conselho cns on cns.isn_conselho = usr.isn_conselho
where doc.isn_internacao_doc = $P{P3}]]>
	</queryString>
