	<queryString>
		<![CDATA[select
      p.num_prontuario,p.dsc_nome,p.num_rg,p.dsc_orgao_emissao_rg,p.num_cep,
       i.dat_cadastro, p.dat_nascimento,
       s.dsc_sexo,i.dsc_endereco,i.dsc_endereco,p.dsc_mae,i.dsc_bairro,
       i.num_ddd_1,i.num_telefone_1,
       i.isn_internacao,
       medpoint.idade2(p.dat_nascimento, i.dat_cadastro) as idade,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 10) as endereco_completo,
       (select vlr_parametro from medpoint.t_parametro where isn_parametro = 13) as dsc_unidade,
       ie.dat_cadastro as dat_cadastro_evolucao,
       ie.dsc_evolucao,
       usrevo.dsc_usuario as usuarioevolucao
from medpoint.t_internacao i
    inner join medpoint.t_paciente p on p.isn_paciente = i.isn_paciente
    inner join medpoint.t_sexo s on s.isn_sexo = p.isn_sexo
    inner join medpoint.t_internacao_evolucao ie on ie.isn_internacao = i.isn_internacao
    inner join seguranca.t_usuario usrevo on usrevo.isn_usuario = ie.isn_usuario
where i.isn_internacao =  $P{p1}
order by ie.isn_internacao_evolucao desc]]>
	</queryString>
