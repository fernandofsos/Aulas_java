	<queryString>
		<![CDATA[SELECT PAC.DSC_NOME AS NOMEPACIENTE, DOC.QTD_DIAS AS NR_DIAS, USR.DSC_USUARIO AS NOMEMEDICO,
USR.NUM_CONSELHO AS NUMCONSELHO, CNS.SGL_CONSELHO AS SGLCONSELHO,DOC.ISN_INTERNACAO_DOC,
(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 10) AS ENDERECO_COMPLETO,
(SELECT VLR_PARAMETRO FROM MEDPOINT.T_PARAMETRO WHERE ISN_PARAMETRO = 13) AS DSC_UNIDADE
FROM MEDPOINT.T_INTERNACAO_DOC AS DOC
LEFT JOIN MEDPOINT.T_INTERNACAO AS I ON I.ISN_INTERNACAO = DOC.ISN_INTERNACAO
LEFT JOIN MEDPOINT.T_PACIENTE AS PAC ON PAC.ISN_PACIENTE = I.ISN_PACIENTE
LEFT JOIN SEGURANCA.T_USUARIO AS USR ON USR.ISN_USUARIO = DOC.ISN_USUARIO
LEFT JOIN SEGURANCA.T_CONSELHO AS CNS ON CNS.ISN_CONSELHO = USR.ISN_CONSELHO
WHERE DOC.ISN_INTERNACAO_DOC = $P{P3}]]>
