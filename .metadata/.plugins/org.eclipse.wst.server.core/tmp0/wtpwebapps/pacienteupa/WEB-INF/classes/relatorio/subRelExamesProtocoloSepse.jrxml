	<field name="dat_cadastro" class="java.sql.Timestamp"/>
	<field name="flg_autorizacao" class="java.lang.String"/>
	<field name="dsc_exame" class="java.lang.String"/>
	<field name="dsc_exame_situacao" class="java.lang.String"/>
