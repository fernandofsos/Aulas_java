<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subRelExamesProtocoloSepse" language="groovy" pageWidth="555" pageHeight="802" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="p1" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select es.isn_exame_solic_aux, es.dat_cadastro, es.flg_autorizacao,
e.dsc_exame,
(est.DSC_EXAME_SITUACAO ||
 CASE WHEN es.flg_autorizacao = 'E' THEN ' (Autorização)'
     WHEN es.flg_autorizacao = 'P' THEN ' (Autorização)'
     WHEN es.flg_autorizacao = 'N' THEN ' (Não Autorizado)'
     ELSE ''
END) AS DSC_EXAME_SITUACAO
from medpoint.t_exame_solic es
left join medpoint.t_exame e on e.isn_exame = es.isn_exame
left join medpoint.T_EXAME_SITUACAO est on est.ISN_EXAME_SITUACAO = es.ISN_EXAME_SITUACAO
where es.isn_protocolo_sepse = $P{p1}::integer]]>
	</queryString>
